			      </geometry>
			      <material name="yellow"/>
		      </collision>
		      
		      <inertial>
			      <origin xyz="0.0 -0.001694 0.042616" rpy="0 0 0"/>
			      <mass value="100"/>	
			      <inertia
				  ixx="0.723" ixy="0.0" ixz="0.0"
				  iyy="0.709" iyz="0.003"
				  izz="1.346"/>
		      </inertial>
