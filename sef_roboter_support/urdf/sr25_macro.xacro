		      <collision>
			      <geometry>
				    <mesh filename="package://sef_roboter_support/meshes/sr25/collision/sr25_axis_5.stl" />
			      </geometry>
			      <material name="yellow" />
			         
		      </collision>
		      <inertial>
			      <origin xyz="-0.000029 -0.074224 -0.055689" rpy="0 0 0"/>
			      <mass value="10"/>	
			      <inertia
				  ixx="0.015" ixy="0.0" ixz="0.0"
				  iyy="0.009" iyz="0.004"
				  izz="0.011"/>
		      </inertial>
	      </link>
	      
	      <link name="${prefix}link_tool">
		      <visual>
			      <geometry>
				    <mesh filename="package://sef_roboter_support/meshes/sr25/visual/sr25_axis_tool.stl" />
			      </geometry>
			      <material name="black" />			         
