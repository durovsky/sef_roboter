				  ixx="0.001" ixy="0.0" ixz="0.0"
				  iyy="0.001" iyz="0.0"
				  izz="0.001"/>
		      </inertial>
	      </link>
	      
	      <!-- end of link list -->
	     
	      <!-- joint list -->
	      <joint name="fixed" type="fixed">
			<parent link="world"/>
			<child link="base_link"/>			
	      </joint>
	         
	      <joint name="${prefix}joint_1" type="revolute">
			<parent link="${prefix}base_link"/>
			<child link="${prefix}link_1"/>
			<origin xyz="0 0 0.075738" rpy="1.5708 0 -1.5745"/>
			<axis xyz="0 -1 0" />
			<limit lower="-2.8" upper="2.8" effort="10000" velocity="1" />			
 			<dynamics damping="0.7" friction="100"/> 
	      </joint>
	      
	      <joint name="${prefix}joint_2" type="revolute">
			<parent link="${prefix}link_1"/>
			<child link="${prefix}link_2"/>
			<origin xyz="0 0.24264 -0.29599" rpy="1.5687 0 0"/>
			<axis xyz="-1 0 0" />
			<limit lower="-0.5" upper="1.5" effort="10000" velocity="1" />
			<dynamics damping="0.7" friction="100"/> 
	      </joint>
	      
	      <joint name="${prefix}joint_3" type="revolute">
			<parent link="${prefix}link_2"/>
			<child link="${prefix}link_3"/>
			<origin xyz="0 0 -0.79724" rpy="0.005529 0 0"/>
			<axis xyz="-1 0 0" />
			<limit lower="-0.5" upper="1.5" effort="10000" velocity="1" />
		        <dynamics damping="0.7" friction="100"/> 
	      </joint>
	      
	      <joint name="${prefix}joint_4" type="revolute">
			<parent link="${prefix}link_3"/>
			<child link="${prefix}link_4"/>
			<origin xyz="0 -0.15811 0" rpy="0 0.0014787 0"/>
			<axis xyz="0 1 0" />
			<limit lower="-1.6" upper="1.6" effort="10000" velocity="1" />
			<dynamics damping="0.7" friction="100"/>
              </joint>
	      
	      <joint name="${prefix}joint_5" type="revolute">
			<parent link="${prefix}link_4"/>
			<child link="${prefix}link_5"/>
			<origin xyz="0 -0.58433 0" rpy="0 0 -0.033693"/>
			<axis xyz="0 0 1" />
			<limit lower="-3" upper="3" effort="10000" velocity="1" />
			<dynamics damping="0.7" friction="100"/>
              </joint>
	      
	      <joint name="${prefix}joint_6" type="revolute">
			<parent link="${prefix}link_5"/>
			<child link="${prefix}link_tool"/>
			<origin xyz="0 -0.164051291252016 -0.00548103448275694" rpy="-1.5707963267949 0 3.14159265358979"/>
			<axis xyz="0 0 -1 " />
			<limit lower="-2.5" upper="2.5" effort="100" velocity="1" />
			<dynamics damping="0.7" friction="100"/>
              </joint>
	      
	      
	      <!-- end of joint list -->  
	      
	       <transmission name="tran_1">
			<type>transmission_interface/SimpleTransmission</type>
			<joint name="joint_1">
			  <hardwareInterface>EffortJointInterface</hardwareInterface>
			</joint>
			<actuator name="motor_1">
			  <hardwareInterface>EffortJointInterface</hardwareInterface>
			  <mechanicalReduction>80</mechanicalReduction>
			</actuator>
	      </transmission>
	      
	      <transmission name="tran_2">
			<type>transmission_interface/SimpleTransmission</type>
			<joint name="joint_2">
